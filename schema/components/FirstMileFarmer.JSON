{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "First mile farm data schema",
	"description": "Data structure for first mile farm data, as part of the reference framework for the collection, storage and exchanget of first mile farm data",
	"language": "en",
	"type": "object",
	"properties": 
	{
		"metaData": 
		{
			"type":"array",
			"title":"Meta data",
			"items":{
				"$ref":"#/definitions/MetaData"
			}
		},
		"farmerGroups": 
		{
			"type":"array",
			"title":"Farmer groups",
			"items":{
				"$ref":"#/definitions/Group"
			}

		},
		"Farmers": 
		{
			"type":"array",
			"title":"Farmers",
			"items":{
				"$ref":"#/definitions/Farmer"
			}
		},
		"Farms": 
		{
			"type":"array",
			"title":"Farm",
			"items":{
				"$ref":"#/definitions/Farm"
			}
		},
		"Plots": 
		{
			"type":"array",
			"title":"Plots",
			"items":{
				"$ref":"#/definitions/Plot"
			}
		},
		"cropObsevations": {
			"type":"array",
			"title":"Crop Observations",
			"items":{
				"$ref":"#/definitions/CropObservation"
			}
		}
	}
}
